@model List<string>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h2>Departments</h2>
@foreach (string dept in Model) {
    <p>@dept</p>
    <div class="card shadowBox">
        <div class="card-body" onclick="RedirectToDept('@dept');">
            <p><b>@dept</b></p>
        </div>
    </div>
    <br />
}