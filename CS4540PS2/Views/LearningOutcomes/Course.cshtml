@{
}

<h1>Course</h1>
@foreach(var x in Model.LearningOutcomes) {
    <p>@x.Name</p>
}
<br />
@foreach(var y in Model.CourseInstance) {
    <p>@y.Name</p>
}
@foreach (var x in Model.EvaluationMetrics) {
    <p>@x.Name</p>
}
@foreach(var x in Model.SampleFiles) {
    <p>@x.FileName</p>
}